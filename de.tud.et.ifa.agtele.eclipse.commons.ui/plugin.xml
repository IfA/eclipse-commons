<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="de.tud.et.ifa.agtele.ui.views.category"
            name="%_UI_ViewCategory_Name">
      </category>
      <view
            category="de.tud.et.ifa.agtele.ui.views.category"
            class="de.tud.et.ifa.agtele.ui.views.EMFModelHelpView"
            icon="icons/help_view.gif"
            id="de.tud.et.ifa.agtele.ui.views.EMFModelHelpView"
            name="%_UI_EMFModelHelpView_Name">
      </view>
   </extension>   
      <extension point="org.eclipse.ui.editors">
      <editor
            class="de.tud.et.ifa.agtele.ui.editors.AgteleEcoreEditor"
            contributorClass="org.eclipse.emf.ecore.presentation.EcoreActionBarContributor"
            default="true"
            extensions="ecore"
            icon="icons/EcoreModelFile.gif"
            id="de.tud.et.ifa.agtele.ui.editors.EcoreEditorID"
            name="AGTele Ecore Model Editor">
         <contentTypeBinding contentTypeId="org.eclipse.emf.ecore"/>
         <contentTypeBinding contentTypeId="org.eclipse.emf.emof"/>
      </editor>
   </extension>
   
   <extension point="org.eclipse.ui.editors">
      <editor
            class="de.tud.et.ifa.agtele.ui.editors.AgteleEcoreEditor"
            contributorClass="org.eclipse.emf.ecore.presentation.EcoreActionBarContributor$Reflective"
            default="true"
            extensions="xmi"
            icon="platform:/plugin/org.eclipse.emf.edit/icons/full/obj16/ModelFile.gif"
            id="de.tud.et.ifa.agtele.ui.editors.ReflectiveEcoreEditorID"
            name="AGTele Reflective Ecore Model Editor">
         <contentTypeBinding contentTypeId="org.eclipse.emf.ecore.xmi"/>
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            class="de.tud.et.ifa.agtele.ui.contributions.EcoreEditorMenuContribution"
            locationURI="popup:de.tud.et.ifa.agtele.ui.editors.EcoreEditorID">
      </menuContribution>
      <menuContribution
            allPopups="false"
            class="de.tud.et.ifa.agtele.ui.contributions.EcoreEditorMenuContribution"
            locationURI="popup:org.eclipse.emf.ecore.presentation.EcoreEditorID">
      </menuContribution>
      <menuContribution
            allPopups="false"
            class="de.tud.et.ifa.agtele.ui.contributions.OpenCodeMenuContribution"
            locationURI="popup:org.eclipse.emf.codegen.ecore.genmodel.presentation.GenModelEditorID?after=open-actions">
      </menuContribution>
      <menuContribution
            allPopups="true"
            class="de.tud.et.ifa.agtele.ui.contributions.CompilationUnitEditorMenuContribution"
            locationURI="popup:org.eclipse.jdt.ui.CompilationUnitEditor.EditorContext">
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="de.tud.et.ifa.agtele.eclipse.commons.ui.ecoreEditorMenuId"
               label="Agtele Settings">
            <command
                  commandId="de.tud.et.ifa.agtele.eclipse.commons.ui.showInheritedEcoreClassFeaturesCommand"
                  label="Show inherited Class Features"
                  style="toggle">
            </command>
            <command
                  commandId="de.tud.et.ifa.agtele.eclipse.commons.ui.showGenericEcoreFeaturesCommand"
                  label="Show Generics"
                  style="toggle">
            </command>
            <menu
                  id="de.tud.et.ifa.agtele.eclipse.commons.ui.changeGeneratedCodeChangedListenerBehaviorMenu"
                  label="React to changed &apos;@generated&apos; methods"
                  tooltip="Customize the behavior of the listener that reacts to manual changes to &apos;@generated&apos; methods.">
            <command
                  commandId="de.tud.et.ifa.agtele.eclipse.commons.ui.changeGeneratedCodeChangedListenerBehaviorCommand"
                  label="User"
                  style="radio"
                  tooltip="On a per-change basis, let the user decide (via a dialog) whether to react to.">
                   <parameter
                     name="org.eclipse.ui.commands.radioStateParameter"
                     value="0">
               </parameter>
            </command>
            <command
                  commandId="de.tud.et.ifa.agtele.eclipse.commons.ui.changeGeneratedCodeChangedListenerBehaviorCommand"
                  label="Automatic (Add NOT tag)"
                  style="radio"
                  tooltip="Automatically react to changes by adding a &apos;NOT&apos; to the &apos;@generated&apos; tag as well as a TODO for the user.">
                   <parameter
                     name="org.eclipse.ui.commands.radioStateParameter"
                     value="1">
               </parameter>
            </command>
            <command
                  commandId="de.tud.et.ifa.agtele.eclipse.commons.ui.changeGeneratedCodeChangedListenerBehaviorCommand"
                  label="Off"
                  style="radio"
                  tooltip="Turn off the listener.">
               <parameter
                     name="org.eclipse.ui.commands.radioStateParameter"
                     value="2">
               </parameter>
            </command>
         </menu>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.OpenGenModelHandler"
            id="de.tud.et.ifa.agtele.ui.commands.OpenGenModel"
            name="OpenGenModel">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.OpenImplModelCodeHandler"
            id="de.tud.et.ifa.agtele.ui.commands.OpenImplModelCode"
            name="OpenImplModelCode">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.OpenInterfaceModelCodeHandler"
            id="de.tud.et.ifa.agtele.ui.commands.OpenInterfaceModelCode"
            name="OpenInterfaceModelCode">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.OpenEditCodeHandler"
            id="de.tud.et.ifa.agtele.ui.commands.OpenEditCode"
            name="OpenEditCode">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.OpenEditorCodeHandler"
            id="de.tud.et.ifa.agtele.ui.commands.OpenEditorCode"
            name="OpenEditorCode">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.OpenFactoryCodeHandler"
            id="de.tud.et.ifa.agtele.ui.commands.OpenFactoryCode"
            name="OpenFactoryCode">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.OpenMetamodelHandler"
            id="de.tud.et.ifa.agtele.ui.commands.OpenMetamodel"
            name="OpenMetamodel">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.ShowInheritedEcoreClassFeaturesCommandHandler"
            id="de.tud.et.ifa.agtele.eclipse.commons.ui.showInheritedEcoreClassFeaturesCommand"
            name="ShowInheritedEcoreClassFeaturesCommand">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.ShowGenericEcoreFeaturesCommandHandler"
            id="de.tud.et.ifa.agtele.eclipse.commons.ui.showGenericEcoreFeaturesCommand"
            name="ShowGenericEcoreFeaturesCommand">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.AddValidationEOperationHandler"
            id="de.tud.et.ifa.agtele.ui.commands.AddValidationEOperation"
            name="AddValidationEOperation">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.PushCodeToEcoreHandler"
            id="de.tud.et.ifa.agtele.ui.commands.PushCodeToEcoreHandler"
            name="PushCodeToEcoreHandler">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.AddDocumentationAnnotationHandler"
            id="de.tud.et.ifa.agtele.ui.commands.AddDocumentationAnnotation"
            name="AddDocumentationAnnotation">
      </command>
      <command
            defaultHandler="de.tud.et.ifa.agtele.ui.handlers.ChangeGeneratedCodeChangedListenerBehaviorHandler"
            id="de.tud.et.ifa.agtele.eclipse.commons.ui.changeGeneratedCodeChangedListenerBehaviorCommand"
            name="ChangeGeneratedCodeChangedListenerBehaviorCommand">
         <commandParameter
               id="org.eclipse.ui.commands.radioStateParameter"
               name="State"
               optional="false">
         </commandParameter>
         <state
               class="org.eclipse.ui.handlers.RadioState:0"
               id="org.eclipse.ui.commands.radioState">
         </state>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.tud.et.ifa.agtele.ui.StartupClass">
      </startup>
   </extension>
</plugin>
